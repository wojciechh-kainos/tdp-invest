<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="1" author="radoslawlandowski">
        <preConditions onFailMessage="The table your trying to load data to doesn't exist!" onFail="HALT">
            <tableExists tableName="unit" schemaName="public"></tableExists>
        </preConditions>
        <loadData encoding="UTF-8"
                        file="dataForChangelogs/newdata.csv"
                        relativeToChangelogFile="true"
                        tableName="unit">
        </loadData>
    </changeSet>
</databaseChangeLog>


<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="1" author="codahale">
        <createTable tableName="people">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="fullName" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="jobTitle" type="varchar(255)"/>
        </createTable>
    </changeSet>


    <changeSet id="1" author="mateusz">
        <loadData tableName="unit" file="assets/staticdata/data1.csv"/>
        <loadData tableName="unit" file="assets/staticdata/data2.csv"/>
        <loadData tableName="unit" file="assets/staticdata/data3.csv"/>
        <loadData tableName="unit" file="assets/staticdata/data4.csv"/>
        <loadData tableName="unit" file="assets/staticdata/data5.csv"/>
        <loadData tableName="unit" file="assets/staticdata/data6.csv"/>
    </changeSet>

</databaseChangeLog>

